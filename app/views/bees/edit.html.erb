<h5>Edit a bee</h5>

<%= form_for @bee do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label "Select Hive" %>
  <%= f.collection_select :hive_id, @hives, :id, :name %>
  <%= f.label "R u a queen ?"%>
  <%= f.check_box :queen %>
  <%= f.submit "Submit" %>
<% end %>
